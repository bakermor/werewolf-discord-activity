{
  "name": "server",
  "version": "0.1.0",
  "description": "The app server",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start:prod",
    "start:prod": "set NODE_ENV=production&& node ./dist/app.js",
    "dev": "nodemon --watch src -e ts,ejs --exec \"npm run start\"",
    "build": "npm run build:clean && npm run build:tsc",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc",
    "debug:start": "npm run build && npm run debug:start:prod",
    "debug:start:prod": "node --nolazy --inspect-brk=9229 ./dist/app.js"
  },
  "author": "Discord",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "nodemon": "^3.1.0",
    "socket.io": "^4.8.1"
  }
}
